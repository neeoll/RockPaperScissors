    </item>
    <item
        android:left="17.5dp"
        android:bottom="17.5dp"
        android:right="17.5dp"
        android:top="17.5dp">
        <shape android:shape="oval">
            <solid android:color="@color/icon_shadow" />
        </shape>
    </item>
    <item
        android:left="17.5dp"
        android:bottom="17.5dp"
        android:right="17.5dp"
        android:top="25dp">
        <shape android:shape="oval">
            <solid android:color="@color/icon_main" />
        </shape>
    </item>
